{
    "name": "phlak/config",
    "description": "Config loading and management",
    "license": "MIT",
    "authors": [
        {
            "name": "Chris Kankiewicz",
            "email": "Chris@ChrisKankiewicz.com"
        }
    ],
    "funding": [
        {
            "type": "github",
            "url": "https://github.com/sponsors/PHLAK"
        },
        {
            "type": "paypal",
            "url": "https://paypal.me/ChrisKankiewicz"
        }
    ],
    "require": {
        "php": ">=7.1",
        "symfony/yaml": "^3.0 || ^4.0 || ^5.0",
        "yosymfony/toml": "^1.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.16.1",
        "phpunit/phpunit": "^7.0 || ^8.0 || ^9.0",
        "psy/psysh": "^0.10",
        "vimeo/psalm": "^3.7"
    },
    "autoload": {
        "psr-4": {
            "PHLAK\\Config\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PHLAK\\Config\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
